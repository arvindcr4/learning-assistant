{
  "dashboard": {
    "id": null,
    "title": "Learning Assistant - Security Monitoring Dashboard",
    "description": "Comprehensive security monitoring dashboard for cybersecurity teams with threat detection, incident response, vulnerability tracking, and security analytics",
    "tags": ["learning-assistant", "security", "threat-detection", "incident-response", "cybersecurity"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["1h", "6h", "12h", "24h", "2d", "7d", "30d", "90d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(learning_assistant_security_incidents[24h])",
          "iconColor": "red",
          "name": "Security Incidents",
          "titleFormat": "Security Incident",
          "textFormat": "Incident: {{incident_type}} - {{severity}}"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(learning_assistant_vulnerability_scans[24h])",
          "iconColor": "orange",
          "name": "Vulnerability Scans",
          "titleFormat": "Vulnerability Scan",
          "textFormat": "Scan completed: {{scan_type}}"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(learning_assistant_security_patches[7d])",
          "iconColor": "green",
          "name": "Security Patches",
          "titleFormat": "Security Patch",
          "textFormat": "Patch applied: {{patch_id}}"
        }
      ]
    },
    "variables": [
      {
        "name": "severity",
        "type": "custom",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [
          { "text": "All", "value": "$__all", "selected": true },
          { "text": "Critical", "value": "critical", "selected": false },
          { "text": "High", "value": "high", "selected": false },
          { "text": "Medium", "value": "medium", "selected": false },
          { "text": "Low", "value": "low", "selected": false }
        ],
        "includeAll": true,
        "multi": true
      },
      {
        "name": "attack_type",
        "type": "query",
        "datasource": "prometheus",
        "query": "label_values(learning_assistant_security_events, attack_type)",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "includeAll": true,
        "multi": true,
        "refresh": 1
      },
      {
        "name": "source_ip_range",
        "type": "query",
        "datasource": "prometheus",
        "query": "label_values(learning_assistant_security_events, source_ip_range)",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "includeAll": true,
        "multi": true,
        "refresh": 1
      }
    ],
    "panels": [
      {
        "id": 1,
        "title": "üõ°Ô∏è Security Status Overview",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "learning_assistant_security_status{severity=~\"$severity\", attack_type=~\"$attack_type\", source_ip_range=~\"$source_ip_range\"}",
            "legendFormat": "Security Status",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": { "color": "red", "index": 0, "text": "COMPROMISED" },
                  "1": { "color": "yellow", "index": 1, "text": "AT RISK" },
                  "2": { "color": "green", "index": 2, "text": "SECURE" }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "yellow", "value": 1 },
                { "color": "green", "value": 2 }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 2,
        "title": "üö® Active Threats",
