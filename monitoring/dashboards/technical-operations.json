{
  "dashboard": {
    "id": null,
    "title": "Learning Assistant - Advanced Technical Operations",
    "description": "Comprehensive technical monitoring dashboard for DevOps, SRE, and Engineering teams with real-time metrics, intelligent alerting, and predictive analytics",
    "tags": ["learning-assistant", "technical", "devops", "sre", "operations", "real-time"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "resets(learning_assistant_restart_count[1h])",
          "iconColor": "red",
          "name": "Service Restarts",
          "titleFormat": "Service Restart",
          "textFormat": "Service restarted"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(learning_assistant_deployment_version[1h])",
          "iconColor": "green",
          "name": "Deployments",
          "titleFormat": "Deployment",
          "textFormat": "New deployment: {{version}}"
        }
      ]
    },
    "variables": [
      {
        "name": "environment",
        "type": "query",
        "datasource": "prometheus",
        "query": "label_values(up{job=\"learning-assistant\"}, environment)",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "includeAll": true,
        "multi": false,
        "refresh": 1
      },
      {
        "name": "instance",
        "type": "query",
        "datasource": "prometheus",
        "query": "label_values(up{job=\"learning-assistant\", environment=\"$environment\"}, instance)",
        "current": {
          "selected": false,
          "text": "All",
          "value": "$__all"
        },
        "options": [],
        "includeAll": true,
        "multi": true,
        "refresh": 1
      },
      {
        "name": "time_range",
        "type": "interval",
        "current": {
          "selected": false,
          "text": "5m",
          "value": "5m"
        },
        "options": [
          { "text": "1m", "value": "1m", "selected": false },
          { "text": "5m", "value": "5m", "selected": true },
          { "text": "15m", "value": "15m", "selected": false },
          { "text": "30m", "value": "30m", "selected": false },
          { "text": "1h", "value": "1h", "selected": false }
        ]
      }
    ],
    "panels": [
      {
        "id": 1,
        "title": "üü¢ System Health Overview",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 0, "y": 0 },
        "targets": [
          {
            "expr": "up{job=\"learning-assistant\", environment=\"$environment\", instance=~\"$instance\"}",
            "legendFormat": "{{instance}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "mappings": [
              {
                "options": {
                  "0": { "color": "red", "index": 0, "text": "DOWN" },
                  "1": { "color": "green", "index": 1, "text": "UP" }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                { "color": "red", "value": 0 },
                { "color": "green", "value": 1 }
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "orientation": "auto",
          "textMode": "auto",
          "colorMode": "background"
        }
      },
      {
        "id": 2,
        "title": "‚ö° Response Time (P95)",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 4, "y": 0 },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(learning_assistant_http_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[$time_range]))",
            "legendFormat": "P95 Response Time",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 0.5 },
                { "color": "orange", "value": 1 },
                { "color": "red", "value": 2 }
              ]
            },
            "unit": "s",
            "decimals": 3
          }
        },
        "options": {
          "colorMode": "background",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 3,
        "title": "üî• Error Rate",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 8, "y": 0 },
        "targets": [
          {
            "expr": "rate(learning_assistant_http_requests_total{status=~\"5..\", environment=\"$environment\", instance=~\"$instance\"}[$time_range]) / rate(learning_assistant_http_requests_total{environment=\"$environment\", instance=~\"$instance\"}[$time_range]) * 100",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 0.5 },
                { "color": "orange", "value": 2 },
                { "color": "red", "value": 5 }
              ]
            },
            "unit": "percent",
            "decimals": 2
          }
        },
        "options": {
          "colorMode": "background",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 4,
        "title": "üë• Active Users",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 12, "y": 0 },
        "targets": [
          {
            "expr": "learning_assistant_active_users{environment=\"$environment\", instance=~\"$instance\"}",
            "legendFormat": "Active Users",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 5,
        "title": "üíæ Memory Usage",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 16, "y": 0 },
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"learning-assistant\", environment=\"$environment\", instance=~\"$instance\"} / 1024 / 1024 / 1024",
            "legendFormat": "Memory (GB)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 1.5 },
                { "color": "orange", "value": 2.5 },
                { "color": "red", "value": 3.5 }
              ]
            },
            "unit": "GB",
            "decimals": 2
          }
        },
        "options": {
          "colorMode": "background",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 6,
        "title": "‚öôÔ∏è CPU Usage",
        "type": "stat",
        "gridPos": { "h": 4, "w": 4, "x": 20, "y": 0 },
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"learning-assistant\", environment=\"$environment\", instance=~\"$instance\"}[$time_range]) * 100",
            "legendFormat": "CPU %",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                { "color": "green", "value": 0 },
                { "color": "yellow", "value": 50 },
                { "color": "orange", "value": 70 },
                { "color": "red", "value": 85 }
              ]
            },
            "unit": "percent",
            "decimals": 1
          }
        },
        "options": {
          "colorMode": "background",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "auto"
        }
      },
      {
        "id": 7,
        "title": "üîÑ Request Rate & Response Time Trends",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
        "targets": [
          {
            "expr": "rate(learning_assistant_http_requests_total{environment=\"$environment\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "RPS - {{method}} {{route}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.50, rate(learning_assistant_http_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[$time_range]))",
            "legendFormat": "P50 Latency",
            "refId": "B",
            "yAxis": 2
          },
          {
            "expr": "histogram_quantile(0.95, rate(learning_assistant_http_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[$time_range]))",
            "legendFormat": "P95 Latency",
            "refId": "C",
            "yAxis": 2
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "unit": "reqps"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "P.*Latency"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "s"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                }
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": [],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "single",
            "sort": "none"
          }
        }
      },
      {
        "id": 8,
        "title": "üìä Resource Utilization Heatmap",
        "type": "heatmap",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
        "targets": [
          {
            "expr": "rate(learning_assistant_http_request_duration_seconds_bucket{environment=\"$environment\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "{{le}}",
            "refId": "A"
          }
        ],
        "options": {
          "calculate": false,
          "calculation": {},
          "cellGap": 2,
          "cellValues": {},
          "color": {
            "exponent": 0.5,
            "fill": "dark-orange",
            "mode": "spectrum",
            "reverse": false,
            "scale": "exponential",
            "scheme": "Spectral",
            "steps": 128
          },
          "exemplars": {
            "color": "rgba(255,0,255,0.7)"
          },
          "filterValues": {
            "le": 1e-9
          },
          "legend": {
            "show": true
          },
          "rowsFrame": {
            "layout": "auto"
          },
          "tooltip": {
            "show": true,
            "yHistogram": false
          },
          "yAxis": {
            "axisPlacement": "left",
            "reverse": false,
            "unit": "s"
          }
        }
      },
      {
        "id": 9,
        "title": "üóÉÔ∏è Database Performance Metrics",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
        "targets": [
          {
            "expr": "rate(learning_assistant_db_query_duration_seconds_sum{environment=\"$environment\", instance=~\"$instance\"}[$time_range]) / rate(learning_assistant_db_query_duration_seconds_count{environment=\"$environment\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "Avg Query Time",
            "refId": "A"
          },
          {
            "expr": "rate(learning_assistant_db_queries_total{environment=\"$environment\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "Query Rate",
            "refId": "B",
            "yAxis": 2
          },
          {
            "expr": "learning_assistant_db_connections_active{environment=\"$environment\", instance=~\"$instance\"}",
            "legendFormat": "Active Connections",
            "refId": "C",
            "yAxis": 2
          },
          {
            "expr": "learning_assistant_db_connections_idle{environment=\"$environment\", instance=~\"$instance\"}",
            "legendFormat": "Idle Connections",
            "refId": "D",
            "yAxis": 2
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "unit": "s"
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": ".*Rate|.*Connections"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "short"
                },
                {
                  "id": "custom.axisPlacement",
                  "value": "right"
                }
              ]
            }
          ]
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 10,
        "title": "üîç Error Analysis & Classification",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
        "targets": [
          {
            "expr": "rate(learning_assistant_http_requests_total{status=~\"4..\", environment=\"$environment\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "4xx Errors - {{status}}",
            "refId": "A"
          },
          {
            "expr": "rate(learning_assistant_http_requests_total{status=~\"5..\", environment=\"$environment\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "5xx Errors - {{status}}",
            "refId": "B"
          },
          {
            "expr": "rate(learning_assistant_errors_total{environment=\"$environment\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "Application Errors - {{type}}",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 20,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "unit": "eps"
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 11,
        "title": "üåê Network & I/O Performance",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
        "targets": [
          {
            "expr": "rate(node_network_receive_bytes_total{device!~\"lo|docker.*\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "Network In - {{device}}",
            "refId": "A"
          },
          {
            "expr": "rate(node_network_transmit_bytes_total{device!~\"lo|docker.*\", instance=~\"$instance\"}[$time_range])",
            "legendFormat": "Network Out - {{device}}",
            "refId": "B"
          },
          {
            "expr": "rate(node_disk_read_bytes_total{instance=~\"$instance\"}[$time_range])",
            "legendFormat": "Disk Read - {{device}}",
            "refId": "C"
          },
          {
            "expr": "rate(node_disk_written_bytes_total{instance=~\"$instance\"}[$time_range])",
            "legendFormat": "Disk Write - {{device}}",
            "refId": "D"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "none",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "linear",
              "lineWidth": 1,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
              "showPoints": "never",
              "spanNulls": false,
              "stacking": {
                "group": "A",
                "mode": "none"
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "unit": "Bps"
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        }
      },
      {
        "id": 12,
        "title": "‚ö†Ô∏è Active Alerts & Incidents",
        "type": "table",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
        "targets": [
          {
            "expr": "ALERTS{job=\"learning-assistant\", environment=\"$environment\", instance=~\"$instance\"}",
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "filterable": true,
              "inspect": false
            },
            "mappings": [
              {
                "options": {
                  "firing": {
                    "color": "red",
                    "index": 0
                  },
                  "pending": {
                    "color": "yellow",
                    "index": 1
                  },
                  "inactive": {
                    "color": "green",
                    "index": 2
                  }
                },
                "type": "value"
              }
            ],
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Time"
            }
          ]
        },
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "__name__": true,
                "job": true
              },
              "indexByName": {
                "Time": 0,
                "alertname": 1,
                "severity": 2,
                "alertstate": 3,
                "instance": 4,
                "summary": 5
              },
              "renameByName": {
                "Time": "Timestamp",
                "alertname": "Alert Name",
                "severity": "Severity",
                "instance": "Instance",
                "alertstate": "State",
                "summary": "Summary"
              }
            }
          }
        ]
      },
      {
        "id": 13,
        "title": "üìà Predictive Analytics - Resource Forecasting",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 28 },
        "targets": [
          {
            "expr": "predict_linear(process_resident_memory_bytes{job=\"learning-assistant\", environment=\"$environment\", instance=~\"$instance\"}[1h], 3600)",
            "legendFormat": "Memory Forecast (+1h)",
            "refId": "A"
          },
          {
            "expr": "predict_linear(rate(process_cpu_seconds_total{job=\"learning-assistant\", environment=\"$environment\", instance=~\"$instance\"}[5m])[1h:5m], 3600) * 100",
            "legendFormat": "CPU Forecast (+1h)",
            "refId": "B"
          },
          {
            "expr": "predict_linear(rate(learning_assistant_http_requests_total{environment=\"$environment\", instance=~\"$instance\"}[5m])[1h:5m], 3600)",
            "legendFormat": "Request Rate Forecast (+1h)",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisLabel": "",
              "axisPlacement": "auto",
              "barAlignment": 0,
              "drawStyle": "line",
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "lineInterpolation": "smooth",
              "lineStyle": {
                "dash": [10, 10],
                "fill": "dash"
              },
              "lineWidth": 2,
              "pointSize": 5,
              "scaleDistribution": {
                "type": "linear"
              },
